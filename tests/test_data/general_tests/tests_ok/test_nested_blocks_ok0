int main()
{
    string c = "ahoj1";
    cout << c << "\n";

    {
        string c = "ahoj2";
        cout << c << "\n";
        {
            string c = "ahoj3";
            cout << c << "\n";
            {
                string c = "ahoj4";
                cout << c << "\n";
                {
                    string c = "ahoj5";
                    cout << c << "\n";
                }
                cout << c << "\n";
            }
            cout << c << "\n";
        }
        cout << c << "\n";
    }
    
    cout << c << "\n";

    return 0;
}

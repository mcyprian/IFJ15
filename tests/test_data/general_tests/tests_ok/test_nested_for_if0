int main()
{
    for(int i = 0 ; i < 5; i = i + 1)
    {
            if( (i+2) / 2 == 1)
            {
                cout << i << "\n";
            }
            else
            {
               for(int j = 0 ; j < 3; j = j + 1)
                {
                
                    cout << j;    
                 }
                 cout << "\n";
            }

    }

    return 42;
}
